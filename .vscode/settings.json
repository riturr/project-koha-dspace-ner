{
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./src/unittest/python",
        "-p",
        "*_tests.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true,
    "python.envFile": "${workspaceFolder}/src/unittest/python/resources/test.env",
